{% extends 'layout.nunjucks' %}

{% block content %}
    <div class="js-page">
        {{ bookPage | safe }}
    </div>

    {% if hasPrevious %}
        <a class="js-previous-page" href=".{{ previousPageUrl }}">Previous Page</a>
    {% endif %}

    {% if hasNext %}
        <a class="js-next-page" href=".{{ nextPageUrl }}">Next Page</a>
    {% endif %}
{% endblock %}
